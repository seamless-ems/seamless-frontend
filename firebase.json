{
  "hosting": {
    "site": "seamless-10a01",
    "public": "dist",
    "rewrites": [
      { "source": "/__/auth/**", "destination": "/__/auth/" },
      { "source": "**", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          }
        ]
      }
    ]
  }
}